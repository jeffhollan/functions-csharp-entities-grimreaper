{
    "version": "2.0",
    "extensions": {
        "durableTask": {
            "storageProvider": {
                "useLegacyPartitionManagement": false
            },
            "tracing": {
                "distributedTracingEnabled": false,
                "distributedTracingProtocol": "W3CTraceContext"
            }
        }
    },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": false
            },
            "httpAutoCollectionOptions": {
                "enableW3CDistributedTracing": true
            }
        }
    }
}